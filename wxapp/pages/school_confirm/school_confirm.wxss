.header{
    text-align: center;
    font-size: 20px;
    color: white;
    padding: 20rpx;
    border-radius: 20rpx;
    width: 50%;
    margin-top: 200rpx;
}
.header image{
    width: 320rpx;
    height: 190rpx;
}
.form{
    width: 90%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    height: 500rpx;
    background-color: rgba(39, 142, 255, 0.3);
    border-radius: 30rpx;
    position: fixed;
    bottom: 80rpx;
}
.input{
    display: flex;
    align-items: center;
    justify-content: center;
    background-color: white;
    border-radius: 10rpx;
    font-size: 18px;
    padding: 10rpx;
    margin: 30rpx;
}
.input input{
    text-align: left;
    margin-left: 10rpx;
}
.input view{
    align-items: center;
    justify-content: center;
    display: flex;
}
.input image{
    width: 50rpx;
    height: 50rpx;
}
.submit{
    background-color: #58d021;
    font-size: 18px;
    border-radius: 20rpx;
    width: 64%;
    margin: 20rpx;
    padding: 5rpx;
    color: #fff;
}
