/* pages/team/team.wxss */
.bg{
    position: absolute;
    width: 100vw;
    height: 100vh;
    background: rebeccapurple;
    z-index: 0;
}
.bg_img{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}
.page{
    position: fixed;
    width: 100vw;
    height: 100vh;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}
.page image{
    width: 80%;
}
